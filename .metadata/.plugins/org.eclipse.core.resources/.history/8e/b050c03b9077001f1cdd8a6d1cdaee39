package br.com.plgs.AppClientes.dto;

import jakarta.validation.constraints.NotBlank;
import jakarta.validation.constraints.NotNull;
import lombok.Getter;
import lombok.Setter;

@Getter
@Setter
public class RegisterUserDto {
	
	@NotBlank(message = "O Username n達o pode ser nulo ou vazio.")
	private String username;
	
	@NotBlank(message = "A senha n達o pode ser nula ou vazia.")
	private String password;
	
	@NotBlank(message = "O nome n達o pode ser nulo ou vazio.")
	private String name;
		
	@NotNull(message = "A role n達o pode ser nula.")
	private String roles;

}
